# Environment Configuration Template
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control

# Funifier API Configuration
FUNIFIER_API_KEY=your_funifier_api_key_here
FUNIFIER_API_SECRET=your_funifier_api_secret_here
FUNIFIER_BASIC_AUTH=your_basic_auth_token_here

# Google Calendar API
GOOGLE_CALENDAR_API_KEY=your_google_calendar_api_key_here

# Application Environment
NODE_ENV=development

# Security Note:
# These credentials should be set as environment variables in your deployment platform
# For Vercel, set them in the dashboard under Settings > Environment Variables